   "source": [
    "class Solution(object):\n",
    "    def reverseList(head):\n",
    "        prev = None\n",
    "        curr = head\n",
    "        \n",
    "        while curr!= None:\n",
    "            tmp = curr.next\n",
    "            curr.next = prev\n",
    "            prev = curr\n",
    "            curr = tmp\n",
    "            \n",
    "        return prev"
   ]
  }
