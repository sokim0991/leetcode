   "source": [
    "class Solution(object):\n",
    "    def invertTree(self, root):\n",
    "        stack = [root]\n",
    "        while stack:\n",
    "            node = stack.pop()\n",
    "            if node:\n",
    "                node.left, node.right = node.right, node.left\n",
    "                stack += node.left, node.right\n",
    "        return root"
   ]
  }
